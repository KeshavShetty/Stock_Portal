<?xml version="1.0" encoding="UTF-8"?>
<application xmlns="http://java.sun.com/xml/ns/javaee" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/application_5.xsd" 
    version="5">

    <display-name>@project.display.name@</display-name>
   
    <module>
        <ejb>@project.ejb.artifact@</ejb>
    </module>

    <module>
        <web>
            <web-uri>@project.web.artifact@</web-uri>
            <context-root>@project.context.root@</context-root>    
        </web>
       
    </module> 
    <module>
     <web>
            <web-uri>@project.web.admin.artifact@</web-uri>
            <context-root>@project.admin.context.root@</context-root>    
        </web>   
   </module>
   
</application>
